layers {
  layer {
    name: "fc11"
    type: "innerproduct"
    num_output: 2500
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 1.
    }
    blobs_lr: 1.
    blobs_lr: 2.
    weight_decay: 1.
    weight_decay: 0.
  }
  bottom: "fc10"
  top: "fc11"
}

layers {
	layer {
		name: "loss"
		type: "so_output_loss"
	}
	bottom: "fc11"
	bottom: "box"
}
